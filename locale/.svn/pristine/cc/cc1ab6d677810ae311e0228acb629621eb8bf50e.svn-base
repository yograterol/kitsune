msgid "You may log in at https://{domain}{login_url}"
msgstr "Voit kirjautua sisään osoitteessa https://{domain}{login_url}"

#. This is an email. Whitespace matters!
#: apps/users/templates/users/email/forgot_username.ltxt:16
msgid "If you did not request this email, there is no need for further action."
msgstr "Ellet pyytänyt tätä sähköpostia, sinun ei tarvitse tehdä mitään."

#. This is at the end of an email.
#. This is an email. Whitespace matters!
#: apps/users/templates/users/email/forgot_username.ltxt:23 apps/users/templates/users/email/pw_reset.ltxt:20
msgid "Thanks,"
msgstr "Kiitos,"

#. This is at the end of an email.
#: apps/users/templates/users/email/forgot_username.ltxt:26 apps/users/templates/users/email/pw_reset.ltxt:23
msgid "The {sitename} team"
msgstr "{sitename} -tiimi"

#. This is an email. Whitespace matters!
#: apps/users/templates/users/email/pw_reset.ltxt:3
msgid "{sitename} Password Reset"
msgstr "{sitename} -sivun salasanan palauttaminen"

#. This is an email. Whitespace matters!
#: apps/users/templates/users/email/pw_reset.ltxt:6
msgid "A request was received to reset the password for this account on %(sitename)s. To change this password please click on the following link, or paste it into your browser's location bar:"
msgstr "Saimme pyynnön nollata salasanan tälle tunnukselle %(sitename)s -sivustolla. Vaihtaaksesi tämän salasanan, napsauta seuraavaa linkkiä tai liitä se selaimesi osoitekenttään:"

#. This is an email. Whitespace matters!
#: apps/users/templates/users/email/pw_reset.ltxt:17
msgid "If you did not request this email there is no need for further action."
msgstr "Ellet pyytänyt tätä sähköpostia, sinun ei tarvitse tehdä mitään."

#: apps/users/templates/users/email/pw_reset_subject.ltxt:1
msgid "Password reset on {sitename}"
msgstr "Salasanan palauttaminen {sitename} -sivustolla"

